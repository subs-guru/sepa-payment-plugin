{
    "name": "subsguru/sepa-payment-plugin",
    "description": "SEPA payment plugin for SubsGuru",
    "type": "cakephp-plugin",
    "repositories": {
        "subsguru/core": {
            "type": "path",
            "url": "/Users/yves/Sites/core",
            "options": {
                "symlink": true
            }
        }
    },
    "require": {
        "php": ">=5.6.0",
        "subsguru/core": "dev-master",
        "digitick/sepa-xml" : "1.1.*",
        "globalcitizen/php-iban": "2.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "SubsGuru\\SEPA\\": "src\\"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SubsGuru\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable" : true
}
